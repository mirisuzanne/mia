---
title: Browse by Tags
eleventyExcludeFromCollections: true
---

{% set tagData = collections | tagData %}

{% if tagData %}
<ol>
  {% for tag in tagData | sortBy('count') | sortBy('top') | reverse %}
    {% if tag.count > 1 %}
      <li>
        <a href="{{ tag.url | url }}">{{ tag.display }}</a>
        ({{ tag.count }} pages)
      </li>
    {% endif %}
  {% endfor %}
</ol>
{% endif %}
