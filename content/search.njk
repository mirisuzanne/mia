---
permalink: search.json
---
{"search" : [
{%- for item in collections.all | getPublic -%}
  {%- set text -%}
    {{ item.data.title }}
    {{ item.data.banner }}
    {{ item.data.sub }}
    {{ item.data.tags }}
    {{ item.templateContent }}
    {% for event in item.data.events %}
      {{ event.title }}
      {{ event.venue }}
    {% endfor %}
  {%- endset -%}
  {
    "url" : "{{ item.url }}",
    "title" : "{{ item.data.title }}",
    "text" : "{{ text | striptags | replace('"', "'") | replace("¯\\_(ツ)_/¯", "[shrug]") | squash }}"
  }{% if not loop.last %},{% else %}{%- endif -%}
{%- endfor -%}
]}
